<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Badges</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_badges.each do |api_v1_badge| %>
      <tr>
        <td><%= link_to 'Show', api_v1_badge %></td>
        <td><%= link_to 'Edit', edit_api_v1_badge_path(api_v1_badge) %></td>
        <td><%= link_to 'Destroy', api_v1_badge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Badge', new_api_v1_badge_path %>
