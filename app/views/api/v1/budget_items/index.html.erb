<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Budget Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_budget_items.each do |api_v1_budget_item| %>
      <tr>
        <td><%= link_to 'Show', api_v1_budget_item %></td>
        <td><%= link_to 'Edit', edit_api_v1_budget_item_path(api_v1_budget_item) %></td>
        <td><%= link_to 'Destroy', api_v1_budget_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Budget item', new_api_v1_budget_item_path %>
