<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Themes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_themes.each do |api_v1_theme| %>
      <tr>
        <td><%= link_to 'Show', api_v1_theme %></td>
        <td><%= link_to 'Edit', edit_api_v1_theme_path(api_v1_theme) %></td>
        <td><%= link_to 'Destroy', api_v1_theme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Theme', new_api_v1_theme_path %>
