<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Csv Files</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_csv_files.each do |api_v1_csv_file| %>
      <tr>
        <td><%= link_to 'Show', api_v1_csv_file %></td>
        <td><%= link_to 'Edit', edit_api_v1_csv_file_path(api_v1_csv_file) %></td>
        <td><%= link_to 'Destroy', api_v1_csv_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csv file', new_api_v1_csv_file_path %>
