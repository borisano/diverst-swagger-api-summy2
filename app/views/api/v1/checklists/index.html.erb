<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Checklists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_checklists.each do |api_v1_checklist| %>
      <tr>
        <td><%= link_to 'Show', api_v1_checklist %></td>
        <td><%= link_to 'Edit', edit_api_v1_checklist_path(api_v1_checklist) %></td>
        <td><%= link_to 'Destroy', api_v1_checklist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Checklist', new_api_v1_checklist_path %>
