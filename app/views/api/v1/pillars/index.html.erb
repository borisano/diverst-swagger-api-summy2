<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Pillars</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_pillars.each do |api_v1_pillar| %>
      <tr>
        <td><%= link_to 'Show', api_v1_pillar %></td>
        <td><%= link_to 'Edit', edit_api_v1_pillar_path(api_v1_pillar) %></td>
        <td><%= link_to 'Destroy', api_v1_pillar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pillar', new_api_v1_pillar_path %>
