<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Base Classes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_base_classes.each do |api_v1_base_class| %>
      <tr>
        <td><%= link_to 'Show', api_v1_base_class %></td>
        <td><%= link_to 'Edit', edit_api_v1_base_class_path(api_v1_base_class) %></td>
        <td><%= link_to 'Destroy', api_v1_base_class, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Base class', new_api_v1_base_class_path %>
