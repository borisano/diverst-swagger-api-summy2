<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Frequency Periods</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_frequency_periods.each do |api_v1_frequency_period| %>
      <tr>
        <td><%= link_to 'Show', api_v1_frequency_period %></td>
        <td><%= link_to 'Edit', edit_api_v1_frequency_period_path(api_v1_frequency_period) %></td>
        <td><%= link_to 'Destroy', api_v1_frequency_period, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

