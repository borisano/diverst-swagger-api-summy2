<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Topics</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_topics.each do |api_v1_topic| %>
      <tr>
        <td><%= link_to 'Show', api_v1_topic %></td>
        <td><%= link_to 'Edit', edit_api_v1_topic_path(api_v1_topic) %></td>
        <td><%= link_to 'Destroy', api_v1_topic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Topic', new_api_v1_topic_path %>
