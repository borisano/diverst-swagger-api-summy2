<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Reward Actions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_reward_actions.each do |api_v1_reward_action| %>
      <tr>
        <td><%= link_to 'Show', api_v1_reward_action %></td>
        <td><%= link_to 'Edit', edit_api_v1_reward_action_path(api_v1_reward_action) %></td>
        <td><%= link_to 'Destroy', api_v1_reward_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

