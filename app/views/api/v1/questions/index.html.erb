<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Questions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_questions.each do |api_v1_question| %>
      <tr>
        <td><%= link_to 'Show', api_v1_question %></td>
        <td><%= link_to 'Edit', edit_api_v1_question_path(api_v1_question) %></td>
        <td><%= link_to 'Destroy', api_v1_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question', new_api_v1_question_path %>
