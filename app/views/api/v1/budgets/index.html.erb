<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Budgets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_budgets.each do |api_v1_budget| %>
      <tr>
        <td><%= link_to 'Show', api_v1_budget %></td>
        <td><%= link_to 'Edit', edit_api_v1_budget_path(api_v1_budget) %></td>
        <td><%= link_to 'Destroy', api_v1_budget, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Budget', new_api_v1_budget_path %>
