<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Initiative Comments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_initiative_comments.each do |api_v1_initiative_comment| %>
      <tr>
        <td><%= link_to 'Show', api_v1_initiative_comment %></td>
        <td><%= link_to 'Edit', edit_api_v1_initiative_comment_path(api_v1_initiative_comment) %></td>
        <td><%= link_to 'Destroy', api_v1_initiative_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Initiative comment', new_api_v1_initiative_comment_path %>
