<h1>Listing Api V1 Enterprises</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_enterprises.each do |api_v1_enterprise| %>
      <tr>
        <td><%= link_to 'Show', api_v1_enterprise %></td>
