<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Expense Categories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_expense_categories.each do |api_v1_expense_category| %>
      <tr>
        <td><%= link_to 'Show', api_v1_expense_category %></td>
        <td><%= link_to 'Edit', edit_api_v1_expense_category_path(api_v1_expense_category) %></td>
        <td><%= link_to 'Destroy', api_v1_expense_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expense category', new_api_v1_expense_category_path %>
