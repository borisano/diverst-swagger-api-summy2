<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Sponsors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_sponsors.each do |api_v1_sponsor| %>
      <tr>
        <td><%= link_to 'Show', api_v1_sponsor %></td>
        <td><%= link_to 'Edit', edit_api_v1_sponsor_path(api_v1_sponsor) %></td>
        <td><%= link_to 'Destroy', api_v1_sponsor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sponsor', new_api_v1_sponsor_path %>
