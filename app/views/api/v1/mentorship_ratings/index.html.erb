<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Mentorship Ratings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_mentorship_ratings.each do |api_v1_mentorship_rating| %>
      <tr>
        <td><%= link_to 'Show', api_v1_mentorship_rating %></td>
        <td><%= link_to 'Edit', edit_api_v1_mentorship_rating_path(api_v1_mentorship_rating) %></td>
        <td><%= link_to 'Destroy', api_v1_mentorship_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mentorship rating', new_api_v1_mentorship_rating_path %>
