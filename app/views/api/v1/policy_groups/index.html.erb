<h1>Listing Api V1 Policy Groups</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_policy_groups.each do |api_v1_policy_group| %>
      <tr>
        <td><%= link_to 'Show', api_v1_policy_group %></td>
        <td><%= link_to 'Edit', edit_api_v1_policy_group_path(api_v1_policy_group) %></td>
        <td><%= link_to 'Destroy', api_v1_policy_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
