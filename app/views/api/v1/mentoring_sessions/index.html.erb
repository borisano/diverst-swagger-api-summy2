<h1>Listing Api V1 Mentoring Sessions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_mentoring_sessions.each do |api_v1_mentoring_session| %>
