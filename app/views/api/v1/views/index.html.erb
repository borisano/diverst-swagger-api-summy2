<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Views</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_views.each do |api_v1_view| %>
      <tr>
        <td><%= link_to 'Show', api_v1_view %></td>
        <td><%= link_to 'Edit', edit_api_v1_view_path(api_v1_view) %></td>
        <td><%= link_to 'Destroy', api_v1_view, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New View', new_api_v1_view_path %>
