<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Campaign Invitations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_campaign_invitations.each do |api_v1_campaign_invitation| %>
      <tr>
        <td><%= link_to 'Show', api_v1_campaign_invitation %></td>
        <td><%= link_to 'Edit', edit_api_v1_campaign_invitation_path(api_v1_campaign_invitation) %></td>
        <td><%= link_to 'Destroy', api_v1_campaign_invitation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Campaign invitation', new_api_v1_campaign_invitation_path %>
