<p id="notice"><%= notice %></p>

<h1>Listing Api V1 News Link Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_news_link_photos.each do |api_v1_news_link_photo| %>
      <tr>
        <td><%= link_to 'Show', api_v1_news_link_photo %></td>
        <td><%= link_to 'Edit', edit_api_v1_news_link_photo_path(api_v1_news_link_photo) %></td>
        <td><%= link_to 'Destroy', api_v1_news_link_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

