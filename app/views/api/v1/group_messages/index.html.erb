<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Group Messages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_group_messages.each do |api_v1_group_message| %>
      <tr>
        <td><%= link_to 'Show', api_v1_group_message %></td>
        <td><%= link_to 'Edit', edit_api_v1_group_message_path(api_v1_group_message) %></td>
        <td><%= link_to 'Destroy', api_v1_group_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

